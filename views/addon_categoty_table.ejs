<%- include('layouts/header'); -%>
    <%- include('layouts/navber'); -%>
      <!-- Main Content -->
      <div class="main-content">
        <section class="section">
          <div class="section-body">
            <div class="row">
              <div class="col-12">
                <div class="card">
                  <div class="card-header">
                    <h4>List Of Addon Category</h4>
                   <a href="/addon_categoty_create" class="ml-auto"><button class="btn btn-info " style="border-radius: 5px;"><i class="fas fa-plus"></i> Add More</button></a>
                  </div>
                  <div class="card-body">
                    <div class="table-responsive">
                      <table class="table table-striped" id="table-1">
                        <thead>
                          <tr class="text-center">
                            <th>Sr.No</th>
                            <th>Category Name</th>
                            <th>Type</th>
                            <th>Edit</th>
                            <th>Delete</th>
                          </tr>
                        </thead>
                        <tbody>
                          <% for(var i=0; i<addonCategory.length;i++) { %>
                          <tr class="text-center">
                            <td><%= i+1; %></td>
                            <td><%= addonCategory[i].name %></td>
                            <td><%= addonCategory[i].type %></td>
                            <td><a href="addon_categoty_edit/<%= addonCategory[i]._id %>" class="btn btn-primary"><i class="fas fa-pencil-alt"></i></a></td>
                            <td><a href="addon_categoty_delete/<%= addonCategory[i]._id %>" class="btn btn-danger"><i class="fas fa-trash-alt"></i></a></td>
                          </tr>
                         <% } %>
                        </tbody>
                      </table>
                    </div>
                  </div>
                </div>
              </div>
            </div>
        
            
          </div>
        </section>
        
      </div>

      

  <%- include('layouts/footer'); -%>


